<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:infra="clr-namespace:Ventas.Infraestructure"
             x:Class="Ventas.App">
    <Application.Resources>
        <ResourceDictionary>
            <!-- Locator -->
            <infra:InstanceLocator x:Key="Locator" />

            <!-- Parameters -->
            <x:String x:Key="UrlAPI">http://www.prowebcol.com/sites/apiventas</x:String>
            <x:String x:Key="Prefix">/api</x:String>
            <x:String x:Key="Controller">/Products</x:String>
            
        </ResourceDictionary>

    </Application.Resources>
</Application>